#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theaninova/Projects/dotio/node_modules/.pnpm/stylelint-config-prettier-scss@1.0.0_stylelint@15.11.0/node_modules/stylelint-config-prettier-scss/bin/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/stylelint-config-prettier-scss@1.0.0_stylelint@15.11.0/node_modules/stylelint-config-prettier-scss/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/stylelint-config-prettier-scss@1.0.0_stylelint@15.11.0/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/theaninova/Projects/dotio/node_modules/.pnpm/stylelint-config-prettier-scss@1.0.0_stylelint@15.11.0/node_modules/stylelint-config-prettier-scss/bin/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/stylelint-config-prettier-scss@1.0.0_stylelint@15.11.0/node_modules/stylelint-config-prettier-scss/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/stylelint-config-prettier-scss@1.0.0_stylelint@15.11.0/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../stylelint-config-prettier-scss/bin/check.js" "$@"
else
  exec node  "$basedir/../stylelint-config-prettier-scss/bin/check.js" "$@"
fi
