#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theaninova/Projects/dotio/node_modules/.pnpm/@sveltejs+kit@1.27.6_svelte@4.2.5_vite@4.5.0/node_modules/@sveltejs/kit/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/@sveltejs+kit@1.27.6_svelte@4.2.5_vite@4.5.0/node_modules/@sveltejs/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/@sveltejs+kit@1.27.6_svelte@4.2.5_vite@4.5.0/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/theaninova/Projects/dotio/node_modules/.pnpm/@sveltejs+kit@1.27.6_svelte@4.2.5_vite@4.5.0/node_modules/@sveltejs/kit/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/@sveltejs+kit@1.27.6_svelte@4.2.5_vite@4.5.0/node_modules/@sveltejs/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/@sveltejs+kit@1.27.6_svelte@4.2.5_vite@4.5.0/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@sveltejs+kit@1.27.6_svelte@4.2.5_vite@4.5.0/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../../../../../../@sveltejs+kit@1.27.6_svelte@4.2.5_vite@4.5.0/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
fi
