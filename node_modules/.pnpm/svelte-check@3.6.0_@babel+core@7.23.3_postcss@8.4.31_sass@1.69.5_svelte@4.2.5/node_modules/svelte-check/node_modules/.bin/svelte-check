#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theaninova/Projects/dotio/node_modules/.pnpm/svelte-check@3.6.0_@babel+core@7.23.3_postcss@8.4.31_sass@1.69.5_svelte@4.2.5/node_modules/svelte-check/bin/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/svelte-check@3.6.0_@babel+core@7.23.3_postcss@8.4.31_sass@1.69.5_svelte@4.2.5/node_modules/svelte-check/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/svelte-check@3.6.0_@babel+core@7.23.3_postcss@8.4.31_sass@1.69.5_svelte@4.2.5/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/theaninova/Projects/dotio/node_modules/.pnpm/svelte-check@3.6.0_@babel+core@7.23.3_postcss@8.4.31_sass@1.69.5_svelte@4.2.5/node_modules/svelte-check/bin/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/svelte-check@3.6.0_@babel+core@7.23.3_postcss@8.4.31_sass@1.69.5_svelte@4.2.5/node_modules/svelte-check/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/svelte-check@3.6.0_@babel+core@7.23.3_postcss@8.4.31_sass@1.69.5_svelte@4.2.5/node_modules:/home/theaninova/Projects/dotio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svelte-check" "$@"
else
  exec node  "$basedir/../../bin/svelte-check" "$@"
fi
